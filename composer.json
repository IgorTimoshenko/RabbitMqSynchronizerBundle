{
    "name": "kimlai/rabbitmq-synchronizer-bundle",
    "description": "easily test your rabbitmq consumers in functional tests",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "Lrqdo\\Bundle\\RabbitMqSynchronizerBundle": "src/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.5",
        "oldsound/rabbitmq-bundle": "v1.5.0"
    },
    "require-dev": {
        "behat/behat": "2.5.*@stable",
        "behat/symfony2-extension": "1.1.2",
        "behat/mink-extension": "1.3.3",
        "behat/mink-browserkit-driver": "1.1.0",
        "phpunit/phpunit": "3.7.*"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "minimum-stability": "stable"
}
